 1729                                               #ifparm \@~text~\@ == \@~1.{:1-:mloop-:text+2}.{:text}~\@
 1730                                                         #temp     :temp-1
 1731                                                 #ifz :temp
 1732                                                         mexit     :1-:mloop-:text+2
 1733                                                 #endif
 1734                                               #endif
