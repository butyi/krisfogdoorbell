 1715                                     #if :temp > 0                                     ;;search from the start
 1716                                                         mdo
 1717                                               #ifparm \@~text~\@ == \@~1.{:mloop}.{:text}~\@
 1718                                                         #temp     :temp-1
 1719                                                 #ifz :temp
 1720                                                         mexit     :mloop
 1721                                                 #endif
 1722                                               #endif
